set linebreak
if(has('gui_running'))
        source ~/.vim/colors/darkblue.vim
	set gfn=DejaVu\ Sans\ Mono\ 12
        set hls
        set number
endif
"highlight OverLength ctermbg=red ctermfg=white guibg=#592929
"match OverLength /\%101v.*/
set tabstop=2
set shiftwidth=2
set backspace=2
set ruler
let foldmethod = "normal"
syntax on
set expandtab
map <MiddleMouse> <Nop>

